pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'build app'
            }
        }
        stage('test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Release') {
            steps {
                echo 'release app'
            }
        }
    }
}
